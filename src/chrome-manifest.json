{
  "name": "ActivityTracker",
  "description": "Track your activity",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage", "activeTab", "tabs"],
  "options_page": "./options.html",
  "background": {
    "scripts": ["./background.min.js"]
  },
  "browser_action": {
    "default_icon": "./images/icon.png"
  }
}
