{
  "name": "ActivityTracker",
  "description": "Track your activity",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "options_page": "./options.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "./popup.html"
  }
}
